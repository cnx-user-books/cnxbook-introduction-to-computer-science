   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mtext> </m:mtext><m:mtext> </m:mtext><m:mtext> </m:mtext><m:mo>=</m:mo><m:mn>200</m:mn><m:mo>+</m:mo><m:mn>50</m:mn><m:mo>+</m:mo><m:mn>4</m:mn><m:mo>+</m:mo><m:mfrac>
      <m:mn>6</m:mn>
