    </m:mrow>
    <m:mn>1</m:mn>
   </m:msup>
   <m:mo>+</m:mo><m:mn>12</m:mn><m:mo>×</m:mo><m:msup>
    <m:mrow>
     <m:mn>16</m:mn>
    </m:mrow>
    <m:mn>0</m:mn>
   </m:msup>
   <m:mo>=</m:mo><m:msub>
    <m:mrow>
     <m:mn>216294</m:mn>
    </m:mrow>
